<h1>Listing clubs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th></th>
  </tr>

<% @clubs.each do |club| %>
  <tr>
    <td><%= club.name %></td>
    <td><%= club.address %></td>
    <td><%= link_to 'Show', club %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Club', new_club_path %>
